<%= form_for [@user, @entry] do |f| %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :content %><br />
    <%= f.text_area :content, :cols => "60", :rows => "20" %>
  </div>
  <%= f.submit "Publish Entry" %>
<% end %>
